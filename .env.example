# System-wide LLM Configuration (cannot be overridden per organization/workspace)
OPENAI_API_KEY=your_openai_api_key_here
DEFAULT_MODEL=gpt-4
DEFAULT_TEMPERATURE=0.1
DEFAULT_MAX_TOKENS=1000

# System-wide Qdrant Configuration (cannot be overridden per organization/workspace)
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# System-wide RAG Configuration (cannot be overridden per organization/workspace)
RAG_COLLECTION_NAME=ai_engine_knowledge_base
RAG_VECTOR_SIZE=1536
RAG_DISTANCE_METRIC=Cosine
RAG_TOP_K=5
RAG_SIMILARITY_THRESHOLD=0.7
DEFAULT_EMBEDDING_MODEL=text-embedding-3-small

# HTTP API Configuration (only configuration that can be overridden per organization/workspace)
DEFAULT_API_TIMEOUT=30
MAX_RETRIES=3

# System-wide Database Configuration (cannot be overridden per organization/workspace)
DATABASE_URL=your_database_connection_string_here
